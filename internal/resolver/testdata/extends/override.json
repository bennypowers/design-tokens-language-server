{
  "$schema": "https://www.designtokens.org/schemas/2025.10.json",
  "base": {
    "color": {
      "$type": "color",
      "$value": {
        "colorSpace": "srgb",
        "components": [1.0, 0, 0],
        "alpha": 1.0
      }
    },
    "spacing": {
      "$type": "dimension",
      "$value": "16px"
    }
  },
  "override": {
    "$extends": "#/base",
    "color": {
      "$type": "color",
      "$value": {
        "colorSpace": "srgb",
        "components": [0, 0, 1.0],
        "alpha": 1.0
      }
    }
  }
}
