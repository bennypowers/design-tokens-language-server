{
  "$schema": "https://www.designtokens.org/schemas/2025.10.json",
  "baseColors": {
    "red": {
      "$type": "color",
      "$value": {
        "colorSpace": "srgb",
        "components": [1.0, 0, 0],
        "alpha": 1.0
      }
    },
    "blue": {
      "$type": "color",
      "$value": {
        "colorSpace": "srgb",
        "components": [0, 0, 1.0],
        "alpha": 1.0
      }
    }
  },
  "themeColors": {
    "$extends": "#/baseColors",
    "green": {
      "$type": "color",
      "$value": {
        "colorSpace": "srgb",
        "components": [0, 1.0, 0],
        "alpha": 1.0
      }
    }
  }
}
