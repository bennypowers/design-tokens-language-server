{
  "space": {
    "$type": "dimension",
    "small": {
      "$value": "4px",
      "$description": "Small space"
    },
    "medium": {
      "$value": "calc(4px * 2)",
      "$description": "Medium space"
    },
    "large": {
      "$value": "16px",
      "$description": "Large space"
    }
  },
  "font": {
    "family": {
      "$value": "'Super Duper', Helvetica, Arial, sans-serif",
      "$description": "Font for headings",
      "$type": "fontFamily"
    },
    "weight": {
      "$value": 400,
      "$description": "Font weight",
      "$type": "fontWeight"
    },
    "notARealColor": {
      "$value": "red",
      "$description": "Not a real color"
    }
  },
  "color": {
    "$type": "color",
    "red": {
      "_": {
        "$value": "red",
        "$description": "Red colour"
      },
      "hex": {
        "$value": "#F00",
        "$description": "Red colour (hex)"
      }
    },
    "green": {
      "$value": "green",
      "$description": "Green colour"
    },
    "ref": {
      "$value": "{color.red._}"
    },
    "blue": {
      "_": {
        "$value": "blue",
        "$description": "Blue colour"
      },
      "dark": {
        "$value": "darkblue",
        "$description": "Dark blue colour"
      },
      "lightdark": {
        "$value": "light-dark(lightblue, darkblue)",
        "$description": "Color scheme color"
      },
      "reference": {
        "$value": "light-dark({color.blue._}, {color.blue.dark})"
      }
    }
  },
  "box-shadow": {
    "$value": "1px 2px 3px 4px rgba(2, 4, 6 / 0.8)",
    "$description": "Box shadow",
    "$type": "boxShadow"
  }
}
