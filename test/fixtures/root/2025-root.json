{
  "$schema": "https://www.designtokens.org/schemas/2025.10.json",
  "color": {
    "primary": {
      "$root": {
        "$type": "color",
        "$value": {
          "colorSpace": "srgb",
          "components": [1.0, 0, 0]
        }
      },
      "hover": {
        "$type": "color",
        "$value": {
          "colorSpace": "srgb",
          "components": [0.8, 0, 0]
        }
      }
    }
  }
}
