{
  "$schema": "https://www.designtokens.org/schemas/2025.10.json",
  "color": {
    "srgb": {
      "$type": "color",
      "$value": {
        "colorSpace": "srgb",
        "components": [1.0, 0.42, 0.21],
        "alpha": 1.0
      }
    },
    "srgbWithHex": {
      "$type": "color",
      "$value": {
        "colorSpace": "srgb",
        "components": [1.0, 0.42, 0.21],
        "alpha": 1.0,
        "hex": "#FF6B35"
      }
    },
    "oklch": {
      "$type": "color",
      "$value": {
        "colorSpace": "oklch",
        "components": [0.68, 0.15, 35],
        "alpha": 1.0
      }
    },
    "withNone": {
      "$type": "color",
      "$value": {
        "colorSpace": "srgb",
        "components": [1.0, "none", 0.21],
        "alpha": 0.8
      }
    }
  }
}
